{
  "name": "@agentlink/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AgentLink - Agent identity and job marketplace on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
